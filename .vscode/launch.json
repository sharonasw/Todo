{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome against localhost",
        "type": "pwa-chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "trace": true,
        "smartStep": true,
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }